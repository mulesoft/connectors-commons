package ${basePackage};

import com.google.common.base.Predicate;

import java.lang.reflect.Method;

public class EndpointNameFilter implements Predicate<Method> {

    private final String endpointName;

    public EndpointNameFilter(String endpointName) {
        this.endpointName = endpointName;
    }

    @Override
    public boolean apply(Method input) {
        return input.getName().equals(endpointName);
    }
}
